{

  "name": "5h-website-app",

  "private": true,

  "version": "0.0.0",

  "proxy": "http://localhost:5144",

  "scripts": {
    "dev:server": "ts-node src/server.ts",
    "dev:client": "vite",
    "dev": "concurrently --kill-others \"npm run dev:client\" \"npm run dev:server\"",

    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "vite build",
    "build": "npm run build:server && npm run build:client",

    "preview": "vite preview",

    "start": "node build/server.ts"
  },

  "dependencies": {

    "body-parser": "^1.20.2",

    "bootstrap": "^5.3.3",

    "cors": "^2.8.5",

    "express": "^4.19.2",

    "googleapis": "^136.0.0",

    "react": "^18.2.0",

    "react-bootstrap": "^2.10.2",

    "react-dom": "^18.2.0"

  },

  "devDependencies": {

    "@types/body-parser": "^1.19.5",

    "@types/cors": "^2.8.17",

    "@types/express": "^4.17.21",

    "@types/react": "^18.0.27",

    "@types/react-dom": "^18.0.10",

    "@vitejs/plugin-react": "^3.1.0",

    "concurrently": "^8.2.2",

    "typescript": "^4.9.5",

    "vite": "^4.1.0"

  }

}

